local library = loadstring(game.HttpGet(game, "https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/0x"))()

local w1 = library:Window("Adopt me - Lunar Event")

local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local collectablesFolder = workspace:WaitForChild("Collectables")
local teleportDelay = 1
local alreadyTeleported = {}
local collectingEnabled = false

local function teleportToModel(model)
    if character and character:FindFirstChild("HumanoidRootPart") then
        local humanoidRootPart = character.HumanoidRootPart
        humanoidRootPart.CFrame = model.PrimaryPart.CFrame
        alreadyTeleported[model] = true
    else
        warn("HumanoidRootPart not found!")
    end
end

local function processNewModel(model)
    if model:IsA("Model") and model.PrimaryPart and not alreadyTeleported[model] then
        teleportToModel(model)
        task.wait(teleportDelay)
    else
        warn("The model " .. model.Name .. " does not have a PrimaryPart or has already been visited!")
    end
end

local function collectModels()
    for _, model in ipairs(collectablesFolder:GetChildren()) do
        processNewModel(model)
    end
end

collectablesFolder.ChildAdded:Connect(function(child)
    if collectingEnabled then
        processNewModel(child)
    end
end)

w1:Toggle( --ty chatgpt
    "Collect Shooting Stars",
    "css",
    false,
    function(toggled)
        collectingEnabled = toggled
        if collectingEnabled then
            collectModels()
        else
            alreadyTeleported = {}
        end
    end
)

w1:Label("YT: PabloScripts21")

w1:Button(
    "Copy Youtube link",
    function()
        setclipboard('https://www.youtube.com/@PabloScripts21')
        game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Notification", Text = "Youtube link copied to clipboard", Button1 = "OK", Duration = 5})
    end
)

w1:Button(
    "Destroy GUI",
    function()
        for i, v in pairs(game.CoreGui:GetChildren()) do
            if v:FindFirstChild("Top") then
                v:Destroy()
            end
        end
    end
)
