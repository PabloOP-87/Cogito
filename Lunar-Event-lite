local a=loadstring(game.HttpGet(game,"https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/0x"))()local b=a:Window("Adopt Me - Lunar Event")b:Toggle("Collect Shooting Stars","css",false,function(c)if c then local d=game.Players.LocalPlayer;local e=d.Character or d.CharacterAdded:Wait()local f=workspace:WaitForChild("Collectables")local g=1;local function h(i)if e and e:FindFirstChild("HumanoidRootPart")then local j=e.HumanoidRootPart;j.CFrame=i.PrimaryPart.CFrame else warn("HumanoidRootPart não encontrado!")end end;for k,i in ipairs(f:GetChildren())do if i:IsA("Model")and i.PrimaryPart then h(i)task.wait(g)else warn("O modelo "..i.Name.." não possui uma PrimaryPart ou não é um Model!")end end else print("Collect Shooting Stars desativado!")end end)b:Button("TP to Secret Spot",function()game.Players.LocalPlayer.Character:PivotTo(CFrame.new(Vector3.new(-248,435,-1405)))end)b:Button("TP to Vending Machine",function()game.Players.LocalPlayer.Character:PivotTo(CFrame.new(Vector3.new(-326,34,-1426)))end)b:Label("YT: PabloScripts21")b:Button("Copy Youtube link",function()setclipboard('https://www.youtube.com/@PabloScripts21')game:GetService("StarterGui"):SetCore("SendNotification",{Title="Notification",Text="Youtube link copied to clipboard",Button1="OK",Duration=5})end)b:Button("Destroy GUI",function()for l,m in pairs(game.CoreGui:GetChildren())do if m:FindFirstChild("Top")then m:Destroy()end end end)
